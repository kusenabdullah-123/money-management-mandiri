<%- include('../_partials/header'); -%>
<!-- ============================================================== -->
<!-- main wrapper -->
<!-- ============================================================== -->
<div class="dashboard-main-wrapper">
  <%- include('../_partials/navbar'); -%> <%- include('../_partials/sidebar');
  -%>
  <!-- wrapper  -->
  <!-- ============================================================== -->
  <div class="dashboard-wrapper">
    <div class="dashboard-ecommerce">
      <div class="container-fluid dashboard-content">
        <!-- ============================================================== -->
        <!-- pageheader  -->
        <!-- ============================================================== -->
        <div class="row">
          <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            <div class="page-header">
              <h2 class="pageheader-title">Laporan Keuangan</h2>
            </div>
          </div>
        </div>
        <!-- ============================================================== -->
        <!-- end pageheader  -->
        <!-- ============================================================== -->

        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
          <div class="card">
            <h5 class="card-header">Laporan</h5>
            <div class="card-body">
              <div class="row">
                <div class="col-xl-5 col-lg-5 col-md-5 col-sm-5 col-5">
                  <div class="form-group row">
                    <label
                      for="from"
                      class="col-3 col-lg-2 col-form-label text-right"
                      >From</label
                    >
                    <div class="col-9 col-lg-10">
                      <input id="from" type="date" class="form-control" />
                    </div>
                  </div>
                </div>
                <div class="col-xl-5 col-lg-5 col-md-5 col-sm-5 col-5">
                  <div class="form-group row">
                    <label
                      for="to"
                      class="col-3 col-lg-2 col-form-label text-right"
                      >To</label
                    >
                    <div class="col-9 col-lg-10">
                      <input id="to" type="date" class="form-control" />
                    </div>
                  </div>
                </div>
                <div class="col-xl-2 col-lg-2 col-md-2 col-sm-2 col-2 p-2">
                  <button type="button" class="btn btn-outline-danger">
                    <i class="fas fa-search"></i>
                  </button>
                </div>
              </div>
              <div class="row">
                <button class="btn btn-outline-primary m-2">
                  <i class="fas fa-print"></i>
                </button>
                <div class="table-responsive">
                  <table
                    id="example"
                    class="table table-striped table-bordered second"
                    style="width: 100%"
                  >
                    <thead>
                      <tr>
                        <th>Name</th>
                        <th>Position</th>
                        <th>Office</th>
                        <th>Age</th>
                        <th>Start date</th>
                        <th>Salary</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr>
                        <td>Tiger Nixon</td>
                        <td>System Architect</td>
                        <td>Edinburgh</td>
                        <td>61</td>
                        <td>2011/04/25</td>
                        <td>$320,800</td>
                      </tr>
                      <tr>
                        <td>Garrett Winters</td>
                        <td>Accountant</td>
                        <td>Tokyo</td>
                        <td>63</td>
                        <td>2011/07/25</td>
                        <td>$170,750</td>
                      </tr>
                    </tbody>
                    <tfoot>
                      <tr>
                        <th>Name</th>
                        <th>Position</th>
                        <th>Office</th>
                        <th>Age</th>
                        <th>Start date</th>
                        <th>Salary</th>
                      </tr>
                    </tfoot>
                  </table>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <%- include('../_partials/footer'); -%>
    </div>
    <!-- ============================================================== -->
    <!-- end wrapper  -->
    <!-- ============================================================== -->
  </div>
  <!-- ============================================================== -->
  <!-- end main wrapper  -->
  <!-- ============================================================== -->
  <!-- Optional JavaScript -->
  <%- include('../_partials/optional-js'); -%>
</div>
